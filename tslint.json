{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-shadowed-variable": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "no-console": [
            false
